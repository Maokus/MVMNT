        "fflate": "^0.8.2",
        "gifuct-js": "^2.1.2",
        "jszip": "^3.10.1",
        "mediabunny": "^1.14.4",
        "opentype.js": "^1.3.4",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-icons": "^5.5.0",
        "react-router-dom": "^6.30.1",
        "seedrandom": "^3.0.5",
        "web-vitals": "^2.1.0",
        "zustand": "^4.5.7"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/node": "^20.0.0",
        "@types/opentype.js": "^1.3.4",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "@types/seedrandom": "^3.0.8",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "autoprefixer": "^10.4.21",
        "jsdom": "^24.0.0",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.5.0",
        "vite": "^5.4.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^1.6.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "*",
        "@rollup/rollup-linux-x64-musl": "*"
    },
    "scripts": {
        "dev": "vite",
        "previewprod": "npm run build -- --base /; npx serve build -s -l 5001",
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest --watch",
        "lint": "npx tsc --noEmit",
        "create-element": "node scripts/create-element.mjs",
        "orphan": "bash -c 'set -a && source .env && npx tsx \"$@\"' --",
        "linuxlock": "npm_config_platform=linux npm_config_arch=x64 npm_config_libc=glibc   npm install --package-lock-only --include=optional"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
